package com.devsuperior.dscommerce.entities;

import java.time.LocalDate;
 

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.OneToMany;
import javax.persistence.Table;

@Entity 
@Table(name="tb_user")

public class User {

	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	private Long id;
	private String name;
	private String emal;
	private String phone;
	private LocalDate birthDate;
	private String passwoerd;
	
	
	@OneToMany (mappedBy="client")
	private List<Order> orders = new ArrayList<>();
	
	
	
	public User() {
		super();
	}


	public User(Long id, String name, String emal, String phone, LocalDate birthDate, String passwoerd) {
		super();
		this.id = id;
		this.name = name;
		this.emal = emal;
		this.phone = phone;
		this.birthDate = birthDate;
		this.passwoerd = passwoerd;
	}


	public Long getId() {
		return id;
	}


	public void setId(Long id) {
		this.id = id;
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public String getEmal() {
		return emal;
	}


	public void setEmal(String emal) {
		this.emal = emal;
	}


	public String getPhone() {
		return phone;
	}


	public void setPhone(String phone) {
		this.phone = phone;
	}


	public LocalDate getBirthDate() {
		return birthDate;
	}


	public void setBirthDate(LocalDate birthDate) {
		this.birthDate = birthDate;
	}


	public String getPasswoerd() {
		return passwoerd;
	}


	public void setPasswoerd(String passwoerd) {
		this.passwoerd = passwoerd;
	}
	
	
	
	
	
	
	
}

 